{
    "name": "Shturval Devspace",
    "id": "shturval-devspace",
    "version": "0.0.3",
    "description": "A devcontainer usefule for deploying shturval and other devops tasks",
    "options": {
        "stc_version": {
            "type": "string",
            "proposals": [
                "2.8.1",                
